<?xml version='1.0' encoding='utf-8'?>
<widget id="com.adaptivemedia.ac" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Age Calculator</name>
    <description>
        Age Calculator
    </description>
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <author email="info@adaptivemedia.in" href="http://adaptivemedia.in">
        Adaptive Media Android Team
    </author>
    <content src="index.html" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <feature name="DatePickerPlugin">
        <param name="android-package" value="com.adaptivemedia.datepicker.DatePickerPlugin" />
    </feature>
    
</widget>
